<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="utf-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
  <link rel="apple-touch-icon" sizes="76x76" href="../assets/img/logos/main-logo.png">
  <link rel="icon" type="image/png" href="../assets/img/logos/main-logo.png">
  <title>
    NFT Click | Dashboard
  </title>
  <!--     Fonts and icons     -->
  <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,400,600,700" rel="stylesheet" />
  <!-- Nucleo Icons -->
  <link href="../assets/css/nucleo-icons.css" rel="stylesheet" />
  <link href="../assets/css/nucleo-svg.css" rel="stylesheet" />
  <!-- Font Awesome Icons -->
  <script src="https://kit.fontawesome.com/3d17848b75.js" crossorigin="anonymous"></script>
  <link href="../assets/css/nucleo-svg.css" rel="stylesheet" />
  <!-- CSS only -->
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
  <!-- JavaScript Bundle with Popper -->
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>
  <!-- CSS Files -->
  <link rel="stylesheet" href="assets/css/custom.css">
  <link id="pagestyle" href="../assets/css/soft-ui-dashboard.css?v=1.0.3" rel="stylesheet" />
</head>

<body class="g-sidenav-show  bg-gray-100">
  <main class="main-content position-relative max-height-vh-100 mt-1 border-radius-lg ">
    <!-- Navbar -->
    <nav class="navbar navbar-bg navbar-main navbar-expand-lg px-0 mx-4 shadow-none border-radius-xl" id="navbarBlur" navbar-scroll="true">
      <div class="py-1 ">
        <div class="collapse position-relative navbar-collapse mt-sm-0 mt-2 me-md-0 me-sm-4" id="navbar">
          <nav aria-label="breadcrumb">
            <div class="nav">
              <a href="/imTheAdminAlright">
                <img class="app-logo" src="assets/img/logos/logo.png" alt="Logo">
                <span class="font-weight-bolder mb-0 text-dark">NFT Click</span>
              </a>
              <a href="/log-out" class="btn btn-danger badge log-out" data-toggle="modal" data-target="#exampleModalCenter">
                <i class="fas fa-sign-out-alt"></i>
              </a>
            </div>
            </nav>
        </div>
      </div>
    </nav>
    <!-- End Navbar -->

    <!-- Main Content -->
    <div class="container-fluid py-4">
      <div class="row">
        <div class="col">
          <div class="card overflow-hidden mb-4">
            <div class="card-header text-uppercase text-center py-3">
            <%  if(typeof users != 'undefined'){ %>
                    <h6>All Users</h6>
            <%  }else{ %>
                    <h6>global</h6>
            <%  } %>
            </div>
            <div class="vertical-menu-2  px-0 card-body ">
          <%  if(typeof users != 'undefined'){  %>
                <div class="row">
                  <div class="col mx-2 text-center">
                <%  users.forEach(function(user){ %>

                       <div class="card my-2">
                         <div class="card-body">
                           <label class="text-uppercase text-center">Email</label>
                           <p>
                             <%=user.email %>
                           </p>
                           <label class="text-uppercase text-center">Sponsored by</label>
                           <p>
                             <%=user.sponsorID %>
                           </p>
                         </div>
                       </div>
                  <%  })  %>
                  </div>
                </div>
          <%  }  %>
          <% if(typeof acTive != 'undefined'){  %>
              <div class="row gx-4">
                <div class="col mx-2 text-center">
              <%  acTive.forEach(function(user){ %>

                     <div class="card my-2">
                       <div class="card-body">
                         <form class="" action="/global-credit" method="post">
                           <label class="text-uppercase text-center">Email</label>
                           <p class=" text-dark text-uppercase text-sm">
                               <%=user.email %>
                             </p>
                           <label class="text-uppercase text-center">Referral</label>
                           <p class=" text-dark text-uppercase text-sm">
                               <%=user.earnings.referral %>
                             </p>
                           <label class="text-uppercase text-center">Autobot</label>
                           <p class=" text-dark text-uppercase text-sm">
                               <%=user.earnings.autobot %>
                             </p>
                           <div class="row">
                             <div class="px-0 ">
                               <input type="text" required name="amount" class="form-control text-center" placeholder="Enter amount">
                             </div>
                             <div class="px-0 ">
                               <input type="text" hidden name="email" class="form-control text-center" value="<%=user.email %>">
                             </div>
                           </div>
                           <br>
                           <div class="">
                             <button type="submit" class="btn btn-success text-uppercase btn-block pt-2" name="button">Credit</button>
                           </div>
                         </form>
                       </div>
                     </div>
                <%  })  %>
                </div>
              </div>
          <%  }  %>
            </div>
            </div>
          </div>
        </div>
      </div>
  </main>
  <div class="fixed-plugin">
    <a class="fixed-plugin-button-left  text-dark position-fixed px-3 py-2" href="/">
      <i class="fa fa-home py-2"> </i>
    </a>

    <a class="fixed-plugin-button-center text-dark position-fixed px-3 py-2" href="/package">
      <i class="fa fa-infinity py-2"> </i>
    </a>
    <a class="fixed-plugin-button-right text-primary position-fixed px-3 py-2" href="/profile">
      <i class="fa fa-user py-2"> </i>
    </a>


  </div>
  <!--   Core JS Files   -->
  <script src="assets/vendor/bootstrap.bundle.min.js"></script>
  <script src="../assets/js/core/popper.min.js"></script>
  <script src="../assets/js/core/bootstrap.min.js"></script>
  <script src="../assets/js/plugins/perfect-scrollbar.min.js"></script>
  <script src="../assets/js/plugins/smooth-scrollbar.min.js"></script>
  <script src="../assets/js/plugins/chartjs.min.js"></script>

  <script>
    var win = navigator.platform.indexOf('Win') > -1;
    if (win && document.querySelector('#sidenav-scrollbar')) {
      var options = {
        damping: '0.5'
      }
      Scrollbar.init(document.querySelector('#sidenav-scrollbar'), options);
    }
  </script>
  <!-- Github buttons -->
  <script async defer src="https://buttons.github.io/buttons.js"></script>
  <!-- Control Center for Soft Dashboard: parallax effects, scripts for the example pages etc -->
  <script src="../assets/js/soft-ui-dashboard.min.js?v=1.0.3"></script>
</body>

</html>
